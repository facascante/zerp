<?php

namespace Api\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Tblrealms
 *
 * @ORM\Table(name="tblRealms", indexes={@ORM\Index(name="fk_tblRealms_tblRealmTypes1_idx", columns={"intRealmTypeID"})})
 * @ORM\Entity
 */
class Tblrealms
{
    /**
     * @var integer
     *
     * @ORM\Column(name="intRealmID", type="integer", nullable=false)
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="IDENTITY")
     */
    private $intrealmid;

    /**
     * @var string
     *
     * @ORM\Column(name="strRealmKey", type="string", length=45, nullable=true)
     */
    private $strrealmkey;

    /**
     * @var string
     *
     * @ORM\Column(name="strRealmSecret", type="string", length=100, nullable=true)
     */
    private $strrealmsecret;

    /**
     * @var integer
     *
     * @ORM\Column(name="intStatus", type="integer", nullable=true)
     */
    private $intstatus = '1';

    /**
     * @var \Api\Entity\Tblrealmtypes
     *
     * @ORM\ManyToOne(targetEntity="Api\Entity\Tblrealmtypes")
     * @ORM\JoinColumns({
     *   @ORM\JoinColumn(name="intRealmTypeID", referencedColumnName="intRealmTypeID")
     * })
     */
    private $intrealmtypeid;


}
